<div id="topScore">
    <table>
      <thead>
        <tr>
          <th>Position</th>
          <th>Account</th>
          <th>Wins</th>
          <th>Losses</th>
        </tr>
      </thead>
      <tbody>
      <% @topScores.each_with_index do |score,index| %>
        <tr>
          <td><%= index+1 %></td>
          <td><%= hideEmail(score[0]) %></td>
          <td><%= score[1] %></td>
          <td><%= score[2] %></td>
        </tr>
      <% end %>
      </tbody>
    </table>
</div>