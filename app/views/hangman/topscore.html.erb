<div id="topScore" ng-controller="topScore" >
    <table>
      <thead>
        <tr>
          <th>Position</th>
          <th>Account</th>
          <th>Wins</th>
          <th>Losses</th>
        </tr>
      </thead>
      <tbody>
        <tr ng-repeat="score in topScore">
          <td>{{ $index+1 }}</td>
          <td>{{score[0]}}</td>
          <td>{{score[1]}}</td>
          <td>{{score[2]}}</td>
        </tr>
      </tbody>
    </table>
</div>